/ Content Row
.row
  .col-lg-3.col-md-6.text-center
    %p#notice= notice
    %table.table
      %thead
        %tr
        %th First name
        %th Last name
        %th Cellphone
        %th{:colspan => "3"}
      %tbody
        %tr
          %td= @contact.fname
          %td= @contact.lname
          %td= @contact.cellphone

= link_to 'Edit', edit_contact_path(@contact)
%br
= link_to 'Back', contacts_path
